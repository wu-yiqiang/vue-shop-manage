\r\nrouter.beforeEach((to,from,next)=>{\r\n  if(to.path===\"/login\") return next()\r\n  const token=window.sessionStorage.getItem(\"vuetoken\")\r\n  if(!token) return next(\"/login\")\r\n  next()\r\n})\r\nexport default router\r\n"]}]}